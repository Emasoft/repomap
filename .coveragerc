[run]
source = repomap
omit =
    */tests/*
    */tmp_repos/*
    */textual/*
    */__pycache__/*
    */.eggs/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def cli
    
[html]
directory = coverage_html_report